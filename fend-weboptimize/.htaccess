# set browser caching

ExpiresActive On
ExpiresByType image/gif "public, max-age=600"
ExpiresByType image/png "public, max-age=600"
ExpiresByType image/jpeg "public, max-age=600"
ExpiresByType image/x-icon "public, max-age=600"
ExpiresByType application/pdf "public, max-age=600"
ExpiresByType application/x-javascript "public, max-age=36000"
ExpiresByType text/plain "public, max-age=600"
ExpiresByType text/css "public, max-age=600"

# end browser caching
